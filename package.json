{
  "name": "vite-plugin-i18n-detector",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.30.0",
  "description": "vite plugin for auto lazy-detect i18n resources",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/ts-template.git"
  },
  "keywords": [
    "vite plugin",
    "i18n",
    "lazyload",
    "detector"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "virtual.d.ts",
    "i18n.d.ts"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "up": "taze -I -w -r",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c -p -t -all"
  },
  "peerDependencies": {
    "i18next": ">=21.0.0",
    "vite": ">=^4.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "^0.0.62",
    "debug": "^4.3.4",
    "depth": "^0.1.1",
    "fs-extra": "^11.1.1",
    "jsonc-simple-parser": "^3.0.0",
    "parse-glob": "^3.0.4",
    "strip-dirs": "^3.0.0",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "1.2.32",
    "@minko-fe/tsconfig": "^1.2.32",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/parse-glob": "^3.0.29",
    "bumpp": "^9.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "8.36.0",
    "i18next": "^22.4.13",
    "pnpm": "^7.30.0",
    "query-string": "^8.1.0",
    "taze": "^0.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
